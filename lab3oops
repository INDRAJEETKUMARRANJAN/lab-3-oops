include<iostream>
using namespace std;
class Part{
    private :
    int x,y;
    public :
    void get();
    void show(int x1,int y1){
        double power;
        x=x1;
        y=y1;
    }
};
void Part::get(){
    double power=1;
    int n;
    cin>>n;
    for(int i=1;i<=n;i++){
        power=power*i;
    }
    cout<<"The Power fxn is "  <<power<<endl;
    
}
int main(){
    Part p;
    p.show(2,3);
    p.get();
    return 0;
}
